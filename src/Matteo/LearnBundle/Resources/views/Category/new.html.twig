{% extends 'MatteoLearnBundle::layout.html.twig' %}

{% block content %}
<h1>Category creation</h1>

<form action="{{ path('learn_category_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button class="btn" type="submit">Create</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('learn_category') }}">
            Back to the list
        </a>
    </li>
</ul>
{% endblock content %}